{
  "name": "GitHub.Expandinizr",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Removes truncating of directory and file names in the GitHub repository browser",
  "homepage_url": "http://github.com/thecodejunkie/github.expandinizr",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "content/inject.css"
      ]
    }
  ]
}